  <GDEF xmlns:web="http://aots.adobe.com/2001/web"
         xmlns:rng="http://relaxng.org/ns/structure/1.0"
         xmlns:ots="http://aots.adobe.com/2001/ots"
         major="1"
         minor="0">
      <glyphClassDef format="any">
         <class classID="1" glyphs="80 81 82 83 84"/>
